

// User function Template for Java

class Solution {
    int sumOfDependencies(ArrayList<ArrayList<Integer>> adj, int V) {
        
        int 
        r =0;
        
        for
        (int 
        i=0;i<V;
        i++){
            
            r
            += adj.get(i).size();
        }
        
        return r ;
    }
};
