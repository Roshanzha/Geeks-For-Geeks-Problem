

//User function Template for Java

class Solution {
    int isPalindrome(String S) {
        // code here
        int start =0;
       int end = S.length()-1;
       while(start<=end){
           if(S.charAt(start)==S.charAt(end)){
               start++;
               end--;
           }
           else{
               return 0;
           }
       }
       return 1;
    }
};
