//User function Template for Java

class Solution {
    public void swapElements(int[] arr, int n) {
        for (int i = 0; i < n - 2; i++) {
            swap(i, i + 2, arr);
        }
    }

    public void swap(int a, int b, int[] arr) {
        int temp = arr[a];
        arr[a] = arr[b];
        arr[b] = temp;
    }
}
